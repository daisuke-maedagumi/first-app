rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read;
      allow write: if request.anth.uid != null;
    }
    match /blogs/{blogId} {
      allow read: if request.anth.uid != null;
      allow write: if request.anth.uid != null;
    }
  }
}